{
  "name": "clockodo",
  "version": "0.0.0-semantically-released",
  "description": "Unofficial Node.js SDK for the Clockodo API",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "node": "./dist/cjs/index.mjs",
    "default": "./dist/esm/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rimraf dist",
    "build:tsc": "run-p build:tsc:*",
    "build:tsc:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:tsc:esm": "tsc -p ./tsconfig.esm.json",
    "build:copy": "copyfiles -f ./src/index.mjs ./dist/cjs/",
    "test": "run-p test:unit test:integration",
    "test:unit": "jest --env node tests/unit/**",
    "test:integration": "jest tests/integration/**",
    "test:lint": "eslint --max-warnings 0 --cache --ext js,ts --ignore-path .gitignore .",
    "posttest": "run-s build test:lint",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peerigon/clockodo.git"
  },
  "keywords": [
    "time tracking",
    "api",
    "sdk"
  ],
  "author": "developers@peerigon.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peerigon/clockodo/issues"
  },
  "homepage": "https://github.com/peerigon/clockodo#readme",
  "dependencies": {
    "axios": "^0.21.4",
    "axios-cache-adapter": "^2.7.3",
    "camelcase-keys": "^7.0.0",
    "qs": "^6.10.1",
    "snakecase-keys": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/qs": "^6.9.7",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-peerigon": "^31.0.2",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "nock": "^13.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-jest-resolver": "^1.1.0",
    "typescript": "^4.4.3"
  },
  "files": [
    "dist",
    "plugins"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json,yml,md}": "prettier --write"
  }
}
